{
  "NLog": {
	"internalLogLevel": "Info",
	"autoReload": true,
	"targets": {
	  "logfile": {
		"type": "File",
		"fileName": "log.txt",
		"layout": "${longdate}|${level}|${message}|${exception:format=tostring}"
	  },
	  "logconsole": {
		"type": "Debugger",
		"layout": "${longdate}|${level}|${message}|${exception:format=tostring}"
	  }
	},
	"rules": [
	  {
		"logger": "*",
		"minLevel": "Trace",
		"writeTo": "logfile,logconsole"
	  }
	]
  }
}